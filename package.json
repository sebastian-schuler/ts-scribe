{
  "name": "ts-scribe",
  "version": "0.5.1",
  "description": "A collection of utility functions and types for TypeScript projects in Node.js",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./array": {
      "import": "./dist/array/index.js",
      "require": "./dist/cjs/array/index.js",
      "types": "./dist/array/index.d.ts"
    },
    "./async": {
      "import": "./dist/async/index.js",
      "require": "./dist/cjs/async/index.js",
      "types": "./dist/async/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "require": "./dist/cjs/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./list": {
      "import": "./dist/list/index.js",
      "require": "./dist/cjs/list/index.js",
      "types": "./dist/list/index.d.ts"
    },
    "./math": {
      "import": "./dist/math/index.js",
      "require": "./dist/cjs/math/index.js",
      "types": "./dist/math/index.d.ts"
    },
    "./object": {
      "import": "./dist/object/index.js",
      "require": "./dist/cjs/object/index.js",
      "types": "./dist/object/index.d.ts"
    },
    "./random": {
      "import": "./dist/random/index.js",
      "require": "./dist/cjs/random/index.js",
      "types": "./dist/random/index.d.ts"
    },
    "./string": {
      "import": "./dist/string/index.js",
      "require": "./dist/cjs/string/index.js",
      "types": "./dist/string/index.d.ts"
    },
    "./system": {
      "import": "./dist/system/index.js",
      "require": "./dist/cjs/system/index.js",
      "types": "./dist/system/index.d.ts"
    },
    "./typeguards": {
      "import": "./dist/typeguards/index.js",
      "require": "./dist/cjs/typeguards/index.js",
      "types": "./dist/typeguards/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "bun run build:clean && bun run build:esm && bun run build:cjs && bun run build:types && bun run build:submodules",
    "build:clean": "rm -rf dist",
    "build:esm": "bun build ./src/index.ts --outdir ./dist --target node --format esm",
    "build:cjs": "bun build ./src/index.ts --outdir ./dist/cjs --target node --format cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "build:submodules": "bun scripts/build-submodules.ts",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "test": "bun test",
    "changeset": "bunx changeset",
    "version-packages": "bunx changeset version",
    "release": "bunx changeset publish"
  },
  "engines": {
    "node": ">=18.0",
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-schuler/ts-scribe.git"
  },
  "keywords": [
    "typescript",
    "node",
    "utility",
    "utils",
    "functions",
    "types"
  ],
  "author": "Sebastian Schuler <contact@sebastian-schuler.com> (https://www.sebastian-schuler.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebastian-schuler/ts-scribe/issues"
  },
  "homepage": "https://github.com/sebastian-schuler/ts-scribe#readme",
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.24.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "bun-types": "latest",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  }
}
